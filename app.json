{
  "expo": {
    "name": "NewsReader",
    "slug": "NewsReader",
    "version": "2.0.0",
    "orientation": "portrait",
    "platforms": [
      "android"
    ],
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "An application to read news!",
    "android": {
      "package": "ru.pesboroda.newsreader",
      "versionCode": 1,
      "permissions": []
    }
  }
}
